class India():
    def capital (self):
        print("New Delhi is the capital of India.")
        
    def languge(self):
        print("Hindi is the most widely spoken languge of India.")
        
    def type(self):
        print("India is a develping country.")
        
class USA():
    def capital(self):
        print("washington, D.C. is the capital of USA.")
         
    def languge(self):
        print("English is the primary languge of USA.")
             
    def type(self):
        print("USA is a develping country.")
obj1=India()
obj2=USA()
obj2.capital()
